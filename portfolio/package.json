    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.*",
    "@babel/preset-flow": "7.*",
    "@babel/preset-react": "7.*",
    "@testing-library/jest-dom": "5.*",
    "@testing-library/react": "12.*",
    "@types/jest": "27.*",
    "@types/node": "17.*",
    "@types/react": "18.*",
    "@types/react-dom": "18.*",
    "@typescript-eslint/eslint-plugin": "5.*",
    "@typescript-eslint/parser": "5.*",
    "babel-jest": "28.*",
    "eslint": "8.*",
    "eslint-config-prettier": "8.*",
    "eslint-plugin-prettier": "4.*",
    "eslint-plugin-react": "7.*",
    "eslint-plugin-react-hooks": "4.*",
    "eslint-plugin-simple-import-sort": "7.*",
    "husky": "7.*",
    "jest": "28.*",
    "jest-environment-jsdom": "28.*",
    "jest-junit": "13.*",
    "jest-transform-stub": "2.*",
    "lint-staged": "11.*",
    "prettier": "2.*",
    "react-scripts": "5.*",
    "ts-jest": "28.*",
    "ts-node": "10.*",
    "typescript": "4.*"
  },
  "scripts": {
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b master -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "lint-staged",
    "lint:fix": "yarn eslint . --fix",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{ts,tsx,css,scss}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
